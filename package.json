{
  "name": "melanin-people-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean": "cd android && gradlew clean  && cd ..",
    "clean-bc": "cd android && gradlew cleanBuildCache  && cd ..",
    "cleanand": "cd android && ./gradlew clean  && cd ..",
    "cleanios": "cd ios && rm -rf ./build && cd ..",
    "podr": "cd ios && rm -rf ./Pods  && cd ..",
    "podi": "cd ios && pod install  && cd ..",
    "log": "react-native log-android",
    "device-list": "adb devices",
    "emulator": "emulator -avd Pixel_2_API_28 -port 5556",
    "test": "jest",
    "all": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules/ && rm -fr $TMPDIR/metro* && npm install && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "react-native run-android",
    "android:build-prod-apk": "(cd android && ./gradlew assembleRelease)",
    "android:build-prod-bundle": "(cd android && ./gradlew bundleRelease)",
    "ios": "react-native run-ios",
    "ios-dev": "ENVFILE=.env.local && react-native run-ios",
    "ios-prod": "ENVFILE=.env.production && react-native run-ios",
    "ios-prod-ipa": "ENVFILE=.env.production && react-native run-ios --configuration=release"
  },
